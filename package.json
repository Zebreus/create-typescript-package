  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/node-fetch": "2",
    "@types/prompts": "^2.0.14",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vercel/ncc": "^0.34.0",
    "@zebreus/resolve-tspaths": "^0.8.4",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.build.json && sed '1s;^;#!/usr/bin/env node\\n;' dist/index.js -i && chmod a+x dist/index.js",
    "format": "prettier .",
    "lint": "eslint --cache && tsc --noEmit",
    "test": "jest",
    "prepack": "rm -rf dist && tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.build.json && sed '1s;^;#!/usr/bin/env node\\n;' dist/index.js -i && chmod a+x dist/index.js",
    "prepublish": "eslint --cache && tsc --noEmit",
    "start": "node --loader ts-node/esm --loader @zebreus/resolve-tspaths/esm 'index.js'"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "application",
    "bin",
    "executable"
  ],
  "bin": {
    "create-typescript-thing": "dist/index.js"
  },
  "dependencies": {
    "chalk": "4",
    "clipboardy": "^3.0.0",
    "create-typescript-thing-lib": "^0.0.5",
    "node-fetch": "2",
    "open": "^8.4.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^4.0.0",
    "which": "^2.0.2",
    "yaml": "^2.1.1"
  }
}
